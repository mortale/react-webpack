{
  "type": "object",
  "properties": {
    "basicGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "基本设置"
      },
      "properties": {
        "title": {
          "title": "字段名称",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "日期",
          "x-validator": {
            "required": true,
            "customFieldsRules": true
          }
        },
        "inheritCode": {
          "title": "字段编码",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-visible": "{{!!$self.value}}",
          "x-component-props": {
            "disabled": true
          }
        },
        "description": {
          "title": "描述",
          "x-decorator": "FormItem",
          "x-component": "TextArea",
          "initialValue": "年、月、日可定义，日期区间可定义",
          "x-component-props": {
            "placeholder": "不超过100个字符",
            "autoSize": {
              "minRows": 2,
              "maxRows": 6
            },
            "width": "100%"
          },
          "x-validator": {
            "max": 100,
            "message": "不超过100个字符"
          }
        },
        "type": {
          "title": "字段类型",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-disabled": "{{$published}}",
          "x-data": {
            "contorlTrigger": "onChange"
          },
          "x-reactions": {
            "effects": [
              "onFieldInputValueChange"
            ],
            "fulfill": {
              "run": "typeEffect($self)"
            }
          },
          "initialValue": "DATE",
          "enum": [
            {
              "value": "TEXT",
              "label": "文本"
            },
            {
              "value": "TEXTAREA",
              "label": "文本域"
            },
            {
              "value": "NUMBER",
              "label": "数值"
            },
            {
              "value": "DATE",
              "label": "日期"
            },
            {
              "value": "RADIO",
              "label": "单项"
            }
          ]
        },
        "tips": {
          "title": "输入提示",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "请选择日期",
          "x-component-props": {
            "placeholder": "请输入"
          }
        },
        "format": {
          "title": "日期类型",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Select",
          "initialValue": "YYYY-MM-DD",
          "x-data": {
            "contorlTrigger": "onChange"
          },
          "x-reactions": {
            "target": "[valueRule>minDate,valueRule>maxDate]",
            "fulfill": {
              "state": {
                "component[1].format": "{{$self.value}}"
              }
            }
          },
          "enum": [
            {
              "value": "YYYY",
              "label": "年"
            },
            {
              "value": "YYYY-MM",
              "label": "年-月"
            },
            {
              "value": "YYYY-MM-DD",
              "label": "年-月-日"
            },
            {
              "value": "YYYY-MM-DD HH:mm",
              "label": "年-月-日 时:分"
            },
            {
              "value": "YYYY-MM-DD HH:mm:ss",
              "label": "年-月-日 时:分:秒"
            }
          ]
        }
      }
    },
    "statusGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "状态"
      },
      "properties": {
        "statusInfo>statusType": {
          "title": "字段状态",
          "x-decorator": "FormItem",
          "x-component": "Radio",
          "x-component-props": {
            "isCompose": true
          },
          "initialValue": "EDITABLE",
          "enum": [
            {
              "label": "普通",
              "value": "EDITABLE"
            },
            {
              "label": "只读",
              "value": "READONLY"
            },
            {
              "label": "隐藏",
              "value": "HIDDEN"
            }
          ],
          "x-reactions": {
            "fulfill": {
              "run": "statusTypeChangeEffect($self,{'READONLY':{'nonEditableInfo':'statusInfo>readonlyInfo'},'HIDDEN':{'nonEditableInfo':'statusInfo>hiddenInfo'}})"
            }
          },
          "x-validator": {
            "customFieldsRules": true
          }
        },
        "statusInfo>readonlyInfo": {
          "title": "默认值",
          "x-decorator": "FormItem",
          "x-component": "NonEditableInfoSetter",
          "x-visible": "{{$values['statusInfo>statusType'] === 'READONLY'}}",
          "x-validator": {
            "customFieldsRules": true
          },
          "x-reactions": {
            "dependencies":["statusInfo>statusType"],
            "when": "{{!$self.visible && $deps[0] === 'READONLY'}}",
            "fulfill": {
              "run": "$self.setValue(statusInfo.nonEditableInfo),$form.notify('onFormInputChange')"
            }
          }
        },
        "statusInfo>hiddenInfo": {
          "title": "默认值",
          "x-decorator": "FormItem",
          "x-component": "NonEditableInfoSetter",
          "x-visible": "{{$values['statusInfo>statusType'] === 'HIDDEN'}}",
          "x-validator": {
            "customFieldsRules": true
          },
          "x-reactions": {
            "dependencies":["statusInfo>statusType"],
            "when": "{{!$self.visible && $deps[0] === 'HIDDEN'}}",
            "fulfill": {
              "run": "$self.setValue(statusInfo.nonEditableInfo),$form.notify('onFormInputChange')"
            }
          }
        }
      }
    },
    "valueRuleGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "校验与错误提示"
      },
      "properties": {
        "required": {
          "title": "必填",
          "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}",
          "x-decorator": "FormItem",
          "x-component": "Checkbox",
          "initialValue": false
        },
        "valueRule>requiredErrorMsg": {
          "title": "错误提示",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "字段必填",
          "default": "字段必填",
          "x-visible": "{{!!$values['required']}}"
        },
        "voidSideLine0": {
          "type": "void",
          "x-component": "SideLine",
          "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}"
        },
        "valueRule>range": {
          "title": "填写范围",
          "x-decorator": "FormItem",
          "x-component": "Checkbox",
          "initialValue": false
        },
        "[valueRule>minDate,valueRule>maxDate]": {
          "title": " ",
          "x-decorator": "FormItem",
          "x-component": "RangeSetter",
          "initialValue":["1980","9999"],
          "x-component-props": {
            "type": "rangepicker"
          },
          "x-hidden": "{{!$values['valueRule>range']}}",
          "x-validator": [
            {
              "rangeSetterValidateRules": true
            }
          ]
        },
        "voidSideLine1": {
          "type": "void",
          "x-component": "SideLine"
        },
        "valueRule>formatErrorMsg": {
          "title": "格式错误提示",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "格式不正确"
        }
      }
    }
  }
}