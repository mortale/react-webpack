{
  "type": "object",
  "properties": {
    "basicGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "基本设置"
      },
      "properties": {
        "title": {
          "title": "字段名称",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "单选",
          "x-validator": {
            "required": true,
            "customFieldsRules": true
          }
        },
        "id": {
          "title": "字段编码",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-visible": "{{!!$self.value}}"
      
        },
        "description": {
          "title": "描述",
          "x-decorator": "FormItem",
          "x-component": "TextArea",
          "x-component-props": {
            "placeholder": "不超过100个字符",
            "autoSize": {
              "minRows": 2,
              "maxRows": 6
            },
            "width": "100%"
          },
          "x-validator": {
            "max": 100,
            "message": "不超过100个字符"
          }
        },
        "type": {
          "title": "字段类型",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-disabled": "{{$published}}",
          "x-data": {
            "contorlTrigger": "onChange"
          },
          "x-reactions": {
            "effects": [
              "onFieldInputValueChange"
            ],
            "fulfill": {
              "run": "typeEffect($self)"
            }
          },
          "initialValue": "SELECT",
          "enum": [
            {
              "value": "TEXT",
              "label": "文本"
            },
            {
              "value": "TEXTAREA",
              "label": "文本域"
            },
            {
              "value": "NUMBER",
              "label": "数值"
            },
            {
              "value": "DATE",
              "label": "日期"
            },
            {
              "value": "SELECT",
              "label": "选择"
            }
          ]
        },
        "placeholder": {
          "title": "输入提示",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "请输入",
          "x-component-props": {
            "placeholder": "请输入"
          }
        }
      }
    },
    "statusGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "状态"
      },
      "properties": {
        "statusInfo>statusType": {
          "title": "字段状态",
          "x-decorator": "FormItem",
          "x-component": "Radio",
          "x-component-props": {
            "isCompose": true
          },
          "initialValue": "EDITABLE",
          "enum": [
            {
              "label": "普通",
              "value": "EDITABLE"
            },
            {
              "label": "只读",
              "value": "READONLY"
            },
            {
              "label": "隐藏",
              "value": "HIDDEN"
            }
          ]
        }
      }
    },
    "associateSetterGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "关联词典表"
      },
      "properties": {
        "statusInfo>editableInfo>associateInfo": {
          "x-decorator": "FormItem",
          "x-component": "AssociateDataSourceSetter",
          "x-decorator-props": {
            "span": 24
          },
          "x-validator": {
            "customFieldsRules": true
          }
        }
      }
    },
    "valueRuleGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}",
      "x-component-props": {
        "title": "校验与错误提示"
      },
      "properties": {
        "valueRule>required": {
          "title": "必填",
          "x-decorator": "FormItem",
          "x-component": "Checkbox",
          "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}",
          "initialValue": false
        },
        "valueRule>requiredErrorMsg": {
          "title": "错误提示",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "字段必填",
          "default": "字段必填",
          "x-visible": "{{!!$values['valueRule>required']}}"
        }
      }
    }
  }
}