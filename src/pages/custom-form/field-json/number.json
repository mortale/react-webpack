{
  "type": "object",
  "properties": {
    "basicGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "基本设置"
      },
      "properties": {
        "title": {
          "title": "字段名称",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "数值",
          "x-validator": {
            "required": true,
            "customFieldsRules": true
          }
        },
        "inheritCode": {
          "title": "字段编码",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-visible": "{{!!$self.value}}",
          "x-component-props": {
            "disabled": true
          }
        },
        "description": {
          "title": "描述",
          "x-decorator": "FormItem",
          "x-component": "TextArea",
          "initialValue": "小数位可定义，上下限可定义",
          "x-component-props": {
            "placeholder": "不超过100个字符",
            "autoSize": {
              "minRows": 2,
              "maxRows": 6
            },
            "width": "100%"
          },
          "x-validator": {
            "max": 100,
            "message": "不超过100个字符"
          }
        },
        "type": {
          "title": "字段类型",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-disabled": "{{$published}}",
          "x-data": {
            "contorlTrigger": "onChange"
          },
          "x-reactions": {
            "effects": [
              "onFieldInputValueChange"
            ],
            "fulfill": {
              "run": "typeEffect($self)"
            }
          },
          "initialValue": "NUMBER",
          "enum": [
            {
              "value": "TEXT",
              "label": "文本"
            },
            {
              "value": "TEXTAREA",
              "label": "文本域"
            },
            {
              "value": "NUMBER",
              "label": "数值"
            },
            {
              "value": "DATE",
              "label": "日期"
            },
            {
              "value": "RADIO",
              "label": "单项"
            }
          ]
        },
        "tips": {
          "title": "输入提示",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "请输入数值",
          "x-component-props": {
            "placeholder": "请输入"
          }
        }
      }
    },
    "statusGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "状态"
      },
      "properties": {
        "statusInfo>statusType": {
          "title": "字段状态",
          "x-decorator": "FormItem",
          "x-component": "Radio",
          "x-component-props": {
            "isCompose": true
          },
          "initialValue": "EDITABLE",
          "enum": [
            {
              "label": "普通",
              "value": "EDITABLE"
            },
            {
              "label": "只读",
              "value": "READONLY"
            },
            {
              "label": "隐藏",
              "value": "HIDDEN"
            }
          ],
          "x-reactions": {
            "fulfill": {
              "run": "statusTypeChangeEffect($self,{'READONLY':{'nonEditableInfo':'statusInfo>readonlyInfo'},'HIDDEN':{'nonEditableInfo':'statusInfo>hiddenInfo'}})"
            }
          },
          "x-validator": {
            "customFieldsRules": true
          }
        },
        "statusInfo>readonlyInfo": {
          "title": "默认值",
          "x-decorator": "FormItem",
          "x-component": "NonEditableInfoSetter",
          "x-visible": "{{$values['statusInfo>statusType'] === 'READONLY'}}",
          "x-validator": {
            "customFieldsRules": true
          },
          "x-reactions": {
            "dependencies":["statusInfo>statusType"],
            "when": "{{!$self.visible && $deps[0] === 'READONLY'}}",
            "fulfill": {
              "run": "$self.setValue(statusInfo.nonEditableInfo),$form.notify('onFormInputChange')"
            }
          }
        },
        "statusInfo>hiddenInfo": {
          "title": "默认值",
          "x-decorator": "FormItem",
          "x-component": "NonEditableInfoSetter",
          "x-visible": "{{$values['statusInfo>statusType'] === 'HIDDEN'}}",
          "x-validator": {
            "customFieldsRules": true
          },
          "x-reactions": {
            "dependencies":["statusInfo>statusType"],
            "when": "{{!$self.visible && $deps[0] === 'HIDDEN'}}",
            "fulfill": {
              "run": "$self.setValue(statusInfo.nonEditableInfo),$form.notify('onFormInputChange')"
            }
          }
        }
      }
    },
    "carryRuleGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "进位规则"
      },
      "properties": {
        "roundingInfo>type": {
          "title": "圆整方式",
          "x-decorator": "FormItem",
          "x-component": "Select",
          "x-component-props": {
            "placeholder": "请选择"
          },
          "initialValue": "ROUND",
          "enum": [
            {
              "label": "向上圆整小数位",
              "value": "CEIL"
            },
            {
              "label": "向下圆整小数位",
              "value": "FLOOR"
            },
            {
              "label": "四舍五入小数位",
              "value": "ROUND"
            },
            {
              "label": "只取整数",
              "value": "INTEGER"
            }
          ]
        },
        "valueRule>decimal": {
          "title": "小数位",
          "x-decorator": "FormItem",
          "x-component": "InputNumber",
          "initialValue": 2,
          "x-visible": "{{$values['roundingInfo>type']!=='INTEGER'}}",
          "x-component-props": {
            "width": "100%",
            "min": 0,
            "max":20
          }
        },
        "example": {
          "title": "示例",
          "x-decorator": "FormItem",
          "x-component": "Input",
          "x-component-props": {
            "disabled": true,
            "style": {
              "textAlign": "right"
            }
          },
          "x-reactions": {
            "dependencies": [
              "valueRule>decimal"
            ],
            "fulfill": {
              "state": {
                "value": "{{'9999'+($deps[0] && $deps[0]!=0?\".\"+Array.from({length:$deps[0]},function(){return 9}).join(\"\"):\"\")}}"
              }
            }
          }
        }
      }
    },
    "valueRuleGroup": {
      "type": "void",
      "x-component": "FormGroup",
      "x-component-props": {
        "title": "校验与错误提示"
      },
      "properties": {
        "required": {
          "title": "必填",
          "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}",
          "x-decorator": "FormItem",
          "x-component": "Checkbox",
          "initialValue": false
        },
        "valueRule>requiredErrorMsg": {
          "title": "错误提示",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "字段必填",
          "default": "字段必填",
          "x-visible": "{{!!$values['required']}}"
        },
        "voidSideLine0": {
          "type": "void",
          "x-component": "SideLine",
          "x-visible": "{{$values['statusInfo>statusType']==='EDITABLE'}}"
        },
        "valueRule>range": {
          "title": "填写范围",
          "x-decorator": "FormItem",
          "x-component": "Checkbox",
          "initialValue": false
        },
        "[valueRule>minValue,valueRule>maxValue]": {
          "title": " ",
          "x-decorator": "FormItem",
          "x-component": "RangeSetter",
          "x-visible": "{{!!$values['valueRule>range']}}",
          "initialValue":["0","100"],
          "x-component-props": {
            "type": "number",
            "placeholder": "不限"
          },
          "x-validator": [
            {
              "rangeSetterValidateRules": true
            }
          ],
          "x-reactions":{
            "dependencies": [
              "valueRule>decimal"
            ],
            "fulfill":{
              "state":{
                "component[1].decimal":"{{$deps[0]}}"
              }
            }
          }
        },
        "voidSideLine1": {
          "type": "void",
          "x-component": "SideLine"
        },
        "valueRule>formatErrorMsg": {
          "title": "格式错误提示",
          "required": true,
          "x-decorator": "FormItem",
          "x-component": "Input",
          "initialValue": "请填写合法数值"
        }
      }
    }
  }
}